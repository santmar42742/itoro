      #coinButtons {
        display: flex;
        flex-direction: column;
        justify-content: start;
      }
      .coin-button {
        width: 110px;
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: row;
        gap: 5px;
        padding: 12px 20px;
        margin: 5px;
        min-width: 8rem;
        border: 1px solid #ccc;
        background-color: #f8f9fa;
        cursor: pointer;
        border-radius: 5px;
        transition: 0.3s;
        box-sizing: border-box;
        box-shadow: rgba(50, 50, 93, 0.25) -4px 6px 0px 0px,
          rgba(0, 0, 0, 0.3) 0px 30px 60px -30px !important;
      }

      .coin-button > img {
        width: auto;
        height: 20px;
      }
      .coin-button.active,
      .coin-button:hover {
        background-color: #7892ff;
        color: white;
        border-color: #7892ff;
      }
      .historyChartCanva {
        width: 70vw;
        border-radius: 2rem;
        padding: 1rem;
        /* box-shadow: rgba(0, 0, 0, 0.45) 0px 25px 20px -20px; */
        box-shadow: rgba(0, 0, 0, 0.15) 2.4px 2.4px 3.2px;
      }
      @media only screen and (max-width: 768px) {
        .historyGraph {
          flex-direction: column-reverse;
          padding: 15rem 0.5rem 2.5rem 0.5rem;
        }
        #coinButtons {
          flex-direction: row;
          justify-content: center;
          flex-wrap: wrap;
        }
        .historyChartCanva {
          width: 100%;
        }
      }

